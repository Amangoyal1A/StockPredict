{
  "name": "light-bootstrap-dashboard-react",
  "version": "2.0.0",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "chartist": "^0.10.1",
    "fontawesome": "^5.6.3",
    "gulp": "^4.0.2",
    "gulp-append-prepend": "^1.0.9",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.2",
    "react-chartist": "^0.14.4",
    "react-dom": "^17.0.2",
    "react-notification-alert": "^0.0.13",
